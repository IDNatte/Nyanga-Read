{
	"name": "nyanga-read-layout",
	"version": "1.0.0",
	"scripts": {
		"python:start": "APP_DEV=\"True\" ./.venv/Scripts/python ./window/app.py",
		"svelte:dev": "vite dev",
		"svelte:build": "vite build",
		"svelte:preview": "vite preview",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"svelte:test:unit": "vitest",
		"svelte:lint": "prettier --plugin-search-dir . --check . && eslint .",
		"svelte:format": "prettier --plugin-search-dir . --write .",
		"python:build": "./.venv/Scripts/pyinstaller Nyanga-Read.spec",
		"python:test": "./.venv/Scripts/python ./window/app.py",
		"app:build": "yarn run svelte:build && yarn run python:build"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.15.1",
		"@tailwindcss/typography": "^0.5.9",
		"@types/cookie": "^0.5.1",
		"@types/lodash": "^4.14.191",
		"@types/marked": "^4.0.8",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lodash": "^4.17.21",
		"marked": "^4.2.12",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-french-toast": "^1.0.4-beta.0",
		"svelte-i18n": "^3.6.0",
		"swiper": "^9.1.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
