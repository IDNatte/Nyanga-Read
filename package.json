{
	"name": "nyanga-read",
	"version": "3.0.3",
	"author": {
		"email": "idnatte@gmail.com",
		"name": "IDNatte",
		"url": "https://github.com/IDNatte/Nyanga-Read"
	},
	"scripts": {
		"svelte:dev": "vite dev",
		"svelte:build": "vite build",
		"svelte:preview": "vite preview",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"svelte:lint": "prettier --plugin-search-dir . --check . && eslint .",
		"svelte:format": "prettier --plugin-search-dir . --write .",
		"python:linux:start": "APP_DEV=\"True\" ./.venv/bin/python ./window/app.py",
		"python:linux:ui:dev": "UI_APP_DEV=\"True\" ./.venv/bin/python ./window/app.py",
		"python:win:start": "APP_DEV=\"True\" ./.venv/Scripts/python ./window/app.py",
		"python:win:ui:dev": "UI_APP_DEV=\"True\" ./.venv/Scripts/python ./window/app.py"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@tailwindcss/typography": "^0.5.9",
		"@types/cookie": "^0.5.1",
		"@types/lodash": "^4.14.196",
		"@types/showdown": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lodash": "^4.17.21",
		"postcss": "^8.4.27",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"showdown": "^2.1.0",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.6",
		"svelte-french-toast": "^1.2.0",
		"svelte-i18n": "^3.7.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7"
	},
	"type": "module"
}
